# EditorConfig is awesome
# https://EditorConfig.org

# Top most editorconfig file
root = true

# Configure Unix style newlines and character set
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.py]
indent_size = 4
indent_style = space

[Makefile]
indent_size = 8
indent_style = tab
